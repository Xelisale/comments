[Unit]
Description=uWSGI instance to serve project api comments
After=network.target

[Service]
User=dev
Group=www-data
WorkingDirectory=/home/dev/api_comments
Environment="PATH=/home/dev/api_comments/venv/bin"
ExecStart=/home/dev/api_comments/venv/bin/uwsgi --ini uwsgi.ini

[Install]
WantedBy=multi-user.target